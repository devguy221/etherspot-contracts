  },
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "scripts": {
    "lint": "eslint ./{test,migrations} --ext .js",
    "build": "node ./scripts/build",
    "initialize": "node ./scripts/initialize",
    "compile": "truffle compile",
    "compile:all": "truffle compile --all",
    "migrate": "truffle migrate --network=default",
    "migrate:reset": "truffle migrate --network=default --reset",
    "test": "./scripts/test.sh --network test",
    "test:log-gas-usage": "cross-env LOG_GAS_USAGE=1 ./scripts/test.sh --network test",
    "test:compile-all": "./scripts/test.sh --network test --compile-all"
  },
  "dependencies": {},
  "devDependencies": {
    "@truffle/hdwallet-provider": "1.0.33",
    "@types/bn.js": "4.11.6",
